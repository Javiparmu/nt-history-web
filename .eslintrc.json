{
  "extends": ["next/core-web-vitals"],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "indent": ["error", 2],
    "quotes": ["error", "single"],
    "semi": ["error", "never"],
    "no-unused-vars": "off",
    "comma-dangle": ["error", "always"],
    "@next/next/no-img-element": "off"
  }
}
